<h2 class="page-header"><%= user.name %></h2>
<h2 class="page-header"><%= user.email %></h2>
<h2 class="page-header"><%= user.password %></h2>
<div class="btn-row">
    <a class="btn btn-primary" href="/users/<%= user.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', { url: `/users/${user.id}` }) %> 
</div>
<% if (photosByUser.length > 0) { %>
    <h2 class="page-header">Photos by User</h2>
    <%- include('../partials/photoGrid', { photos: photosByUser }) %>
<% } %> 