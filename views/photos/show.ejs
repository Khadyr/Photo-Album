<h2 class="page-header"><%= photo.title %> </h2>
<img width="100" height="150" src="<%= photo.coverImagePath %> ">
<div>User: <%= photo.user.name %></div>
<div>Publish Date: <%= photo.publishDate.toDateString() %></div>
<div>Page Count: <%= photo.pageCount %></div>
<div>Description: <%= photo.description %></div>

<a href="/photos/<%= photo.id %>/edit">Edit</a>
<%- include('../partials/deleteForm', { url: `/photos/${photo.id}` }) %>
<a href="/users/<%= photo.user.id %>">View User</a>